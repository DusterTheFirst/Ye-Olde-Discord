{
    "cmake.configureOnOpen": false,
    "cmake.statusbar.advanced": {
        "debug": {
            "visibility": "hidden"
        },
        "launch": {
            "visibility": "hidden"
        },
        "build": {
            "visibility": "hidden"
        },
        "buildTarget": {
            "visibility": "hidden"
        }
    },
    "cSpell.words": [
        "ABRT",
        "DHCPS",
        "fauxne",
        "llhttp",
        "multicore",
        "recved"
    ],
    "git.autoRepositoryDetection": false,
    "files.associations": {
        "*.sxcu": "json",
        "**/.cargo/config": "toml",
        "*.pio": "platformio-debug.asm",
        "*.njk": "nunjucks",
        "*.h": "c",
        "*.c": "c",
        "string_view": "c",
        "system_error": "c"
    }
}
